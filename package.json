{
  "name": "atomic-tweetdeck",
  "productName": "Atomic TweetDeck",
  "version": "0.13.1",
  "license": "MIT",
  "author": {
    "name": "Doğan Çelik"
  },
  "main": "index.js",
  "electronVersion": "8.5.2",
  "scripts": {
    "clean": "build.bat clean",
    "start": "electron .",
    "build": "build.bat build",
    "zip:zip": "build.bat zip",
    "zip:tar": "build.bat tar",
    "zip:osx": "npm run zip:tar -- darwin",
    "zip:lin": "npm run zip:tar -- linux",
    "zip:win": "npm run zip:zip -- win32",
    "zip": "npm run zip:osx && npm run zip:lin && npm run zip:win"
  },
  "dependencies": {
    "configstore": "^5.0.1"
  },
  "devDependencies": {
    "electron": "^8.5.2",
    "electron-packager": "^15.0.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "node": true
    },
    "globals": {
      "config": true
    }
  }
}
